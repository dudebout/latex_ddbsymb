\newcommand{\intervalus}[3][cc]{
  \ifthenelse{\equal{#1}{cc}}{\brackets*{#2 , #3}}{}
  \ifthenelse{\equal{#1}{oo}}{\parentheses*{#2 , #3}}{}
  \ifthenelse{\equal{#1}{co}}{\bratheses*{#2 , #3}}{}
  \ifthenelse{\equal{#1}{oc}}{\parenckets*{#2 , #3}}{}
}
\newcommand{\intervalfr}[3][cc]{
  \ifthenelse{\equal{#1}{cc}}{\brackets*{#2 , #3}}{}
  \ifthenelse{\equal{#1}{oo}}{\invertbrackets*{#2 , #3}}{}
  \ifthenelse{\equal{#1}{co}}{\lbrackets*{#2 , #3}}{}
  \ifthenelse{\equal{#1}{oc}}{\rbrackets*{#2 , #3}}{}
}
